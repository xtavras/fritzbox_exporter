{
    "labelRenames": [
        {
            "matchRegex": "(?i)prozessor",
            "renameLabel": "CPU"
        },
        {
            "matchRegex": "(?i)system",
            "renameLabel": "System"
        },
        {
            "matchRegex": "(?i)DSL",
            "renameLabel": "DSL"
        },
        {
            "matchRegex": "(?i)FON",
            "renameLabel": "Phone"
        },
        {
            "matchRegex": "(?i)WLAN",
            "renameLabel": "WLAN"
        },
        {
            "matchRegex": "(?i)USB",
            "renameLabel": "USB"
